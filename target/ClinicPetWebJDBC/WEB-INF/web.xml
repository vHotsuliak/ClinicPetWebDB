<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>ClientViewServlet</servlet-name>
    <servlet-class>com.servlets.ClientViewServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddClientServlet</servlet-name>
    <servlet-class>com.servlets.AddClientServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditClientServlet</servlet-name>
    <servlet-class>com.servlets.EditClientServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteClientServlet</servlet-name>
    <servlet-class>com.servlets.DeleteClientServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchServlet</servlet-name>
    <servlet-class>com.servlets.SearchClientServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ClientViewServlet</servlet-name>
    <url-pattern>/client/view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddClientServlet</servlet-name>
    <url-pattern>/client/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditClientServlet</servlet-name>
    <url-pattern>/client/edit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteClientServlet</servlet-name>
    <url-pattern>/client/delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchServlet</servlet-name>
    <url-pattern>/client/search</url-pattern>
  </servlet-mapping>
</web-app>
